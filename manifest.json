 {
     "manifest_version":2,
     "name":"Rajdeep Launcher",
     "description":"Scan My Website",
     "version":"1.0.0",
     "icons":{
         "128":"icon_128.png"
     },
     "content_scripts":[
         {
             "matches":[
               "<all_urls>"
             ],
             "js":["content.js"]
         }
     ],
     "background":{
        "scripts" : ["background.js"]
     },
     "browser_action":{
         "default_icon": "icon.png",
         "default_popup":"popup.html"
     },
     "permissions":["tabs"]
 }